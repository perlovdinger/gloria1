<?xml version="1.0" encoding="UTF-8"?>
<!-- edited with XMLSpy v2010 (http://www.altova.com) by ï»¿VOLVO INFORMATION TECH AB (VOLVO INFORMATION TECH AB) -->
<xsd:schema xmlns="http://finance.group.volvo.com/common/1_0" xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="http://finance.group.volvo.com/common/1_0" version="1.0.0">
	<xsd:simpleType name="AssetClassType">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="8"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="AssetNumberType">
		<xsd:annotation>
			<xsd:documentation>This is the number which, together with the asset sub-number, identifies a fixed asset in Asset Accounting.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="12"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="AssetSubNumberType">
		<xsd:annotation>
			<xsd:documentation>This is the number which, together with the main asset number, uniquely identifies a fixed asset in Asset Accounting.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="4"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="BankKeyType">
		<xsd:annotation>
			<xsd:documentation>Bank Key number</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="15"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="BankAccountType">
		<xsd:annotation>
			<xsd:documentation>Bank Account number</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="18"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="BusinessAreaType">
		<xsd:annotation>
			<xsd:documentation>A unit in an enterprise that groups product and market combinations as homogeneously as possible for the purpose of developing unified business policy.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="4"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="BusinessPartnerType">
		<xsd:annotation>
			<xsd:documentation>Gives an alphanumeric key, which clearly identifies the customer within Account Receivable.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="10"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ChartOfAccountType">
		<xsd:annotation>
			<xsd:documentation>Key that uniquely identifies a chart of accounts.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="4"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="CorporateGroupType">
		<xsd:annotation>
			<xsd:documentation>Subdivision of the companies in the Business Directory.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="10"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="CompanyCodeType">
		<xsd:annotation>
			<xsd:documentation>The company code is an organizational unit within financial accounting.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="4"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ControllingAreaType">
		<xsd:annotation>
			<xsd:documentation>The controlling area is the highest organizational unit in Controlling.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="4"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="CostCenterType">
		<xsd:annotation>
			<xsd:documentation>Key which together with the controlling area uniquely identifies a cost center.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="10"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="CostElementType">
		<xsd:annotation>
			<xsd:documentation>Items in a chart of accounts that are relevant to cost, are referred to as cost elements.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="10"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="CountryKeyType">
		<xsd:annotation>
			<xsd:documentation>The two-character ISO code in accordance with ISO 3166, which is delivered by SAP as a default.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="3"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="CreditControlAreaType">
		<xsd:annotation>
			<xsd:documentation>The credit control area is an organizational entity which grants and monitors a credit limit for customers.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="4"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="CreditSegmentType">
		<xsd:annotation>
			<xsd:documentation>Gives an alphanumeric key, which clearly identifies the customer within Account Receivable.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="10"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="CurrencyCodeType">
		<xsd:annotation>
			<xsd:documentation>ISO standard</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string"/>
	</xsd:simpleType>
	<xsd:simpleType name="LanguageKeyType">
		<xsd:annotation>
			<xsd:documentation>ISO standard</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string"/>
	</xsd:simpleType>
	<xsd:simpleType name="CustomerType">
		<xsd:annotation>
			<xsd:documentation>Gives an alphanumeric key, which clearly identifies the customer within Account Receivable.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="10"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="DocumentNumberType">
		<xsd:annotation>
			<xsd:documentation>The document number is the key the system uses to access the accounting document. The document number is unique per company code and fiscal year.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="10"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="DocumentTypeType">
		<xsd:annotation>
			<xsd:documentation>Attributes that control the entry of the fiannce document.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="2"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="FiscalYearType">
		<xsd:annotation>
			<xsd:documentation>Period, 12 months as a rule, for which the company is to create its inventory and balance sheet</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="4"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="FunctionalAreaType">
		<xsd:annotation>
			<xsd:documentation>Functional Area. Structure criteria for an enterprise or government based on functional aspects.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="16"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="FunctionalAreaShortType">
		<xsd:annotation>
			<xsd:documentation>Functional Area Short. Structure criteria for an enterprise or government based on functional aspects.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="4"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="GLAccountType">
		<xsd:annotation>
			<xsd:documentation>The G/L account number identifies the G/L account in a chart of accounts.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="10"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="IndustryType">
		<xsd:annotation>
			<xsd:documentation>An industry is a distinct group of companies with the same basic business activity.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="4"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="InternalOrderType">
		<xsd:annotation>
			<xsd:documentation>Key which together with the controlling area uniquely identifies an order.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="12"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="LatitudeType">
		<xsd:restriction base="xsd:decimal">
			<xsd:fractionDigits value="12"/>
			<xsd:totalDigits value="15"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="LongitudeType">
		<xsd:restriction base="xsd:decimal">
			<xsd:totalDigits value="15"/>
			<xsd:fractionDigits value="12"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="MarketType">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="3"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="MaterialNumberType">
		<xsd:annotation>
			<xsd:documentation>Alphanumeric key uniquely identifying the material.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="18"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="OrganizationUnitType">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="4"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PaymentTermType">
		<xsd:annotation>
			<xsd:documentation>Key for defining payment terms composed of cash discount percentages and payment periods.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="4"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PlaceType">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="7"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PlantType">
		<xsd:annotation>
			<xsd:documentation>Key uniquely identifying a plant.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="4"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ProfitCenterType">
		<xsd:annotation>
			<xsd:documentation>Key which together with the controlling area uniquely identifies a profit center.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="10"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="RefDocumentNumberType">
		<xsd:annotation>
			<xsd:documentation>The document number is the key the system uses to access the accounting document. The document number is unique per company code and fiscal year.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="16"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="TaxCodeType">
		<xsd:annotation>
			<xsd:documentation>Tax codes are unique per country. The tax rate calculation rules and further features are unique for each tax code.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="2"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="TradingPartnerType">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="6"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="UserIdType">
		<xsd:annotation>
			<xsd:documentation>SAP User Id. Following Volvo VCD namnings.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="12"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="VATRegistrationNumberType">
		<xsd:annotation>
			<xsd:documentation>VAT reg.no. of the customer, vendor or your company code.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="20"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="VendorType">
		<xsd:annotation>
			<xsd:documentation>Gives an alphanumeric key, which clearly identifies the vendor within Account Payable.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="10"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="VendorInvoiceDetailsType">
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="10"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="WBSElementType">
		<xsd:annotation>
			<xsd:documentation>Work Breakdown Structure Element (WBS Element)</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="24"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="MsgCodeType">
		<xsd:annotation>
			<xsd:documentation>Technical attribute-MessageCode</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="3"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="MsgFunctionType">
		<xsd:annotation>
			<xsd:documentation>Technical attribute-MessageFunction</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="3"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="RefMessageType">
		<xsd:annotation>
			<xsd:documentation>Technical attribute-ReferenceMessage</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="14"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="MessageHeaderType">
		<xsd:sequence>
			<xsd:element name="MsgVersion" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Msg version with all 3 digits, i.e. 1.0.2</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Sender">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="LogicalId" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>Name of sending application. Should normally be the short name in Volvo Group Application Catalog</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="ComponentId" type="xsd:string" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Component id can be used to provide more details about the sender. It can for instance cover FileId and FeederId when needed or specify a subsystem of the sender. </xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="Receiver" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="LogicalId" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>Name of the receiving application. Should normally be the short name in Volvo Group Application Catalog</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="ComponentId" type="xsd:string" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Component id can be used to provide more details about the receiver. It can for instance be a specific instance or subsystem of the application. </xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="CreationDateTime" type="xsd:dateTime">
				<xsd:annotation>
					<xsd:documentation>Date and time when the message was created</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="MessageType">
		<xsd:sequence>
			<xsd:element name="MessageCode" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Error or Warning message code, making it possible to translate message</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="MessageText">
				<xsd:annotation>
					<xsd:documentation>Error or warning message text, normally in english</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="xsd:string">
							<xsd:attribute name="lang" type="xsd:string">
								<xsd:annotation>
									<xsd:documentation>ISO code language of text if not default. Needed?</xsd:documentation>
								</xsd:annotation>
							</xsd:attribute>
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="MessageParameters" maxOccurs="unbounded">
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="xsd:string">
							<xsd:attribute name="name" type="xsd:string"/>
						</xsd:extension>
					</xsd:simpleContent>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ProcessType">
		<xsd:sequence>
			<xsd:element name="MessageHeader" type="MessageHeaderType"/>
			<xsd:element name="Action" type="xsd:string" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attribute name="Attribute1" type="xsd:string">
			<xsd:annotation>
				<xsd:documentation>Optional attribute</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="Attribute2">
			<xsd:annotation>
				<xsd:documentation>Optional attribute</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="Attribute3">
			<xsd:annotation>
				<xsd:documentation>Optional attribute</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="SyncType">
		<xsd:sequence>
			<xsd:element name="MessageHeader" type="MessageHeaderType"/>
			<xsd:element name="Sequence" type="SequenceType" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attribute name="ReasonCode" type="xsd:string">
			<xsd:annotation>
				<xsd:documentation>Indicates the reason for issuing the Sync message, e.g. InitialLoad, Updates</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="ShowType">
		<xsd:sequence>
			<xsd:element name="MessageHeader" type="MessageHeaderType"/>
			<xsd:element name="Result" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>The result of the processing of the request. Success, Warning, Failure, PartialFailure</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Message" type="MessageType" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="NounHeaderType">
		<xsd:sequence>
			<xsd:element name="Action" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates the action to be performed, i.e. Create, Update, Delete, Load</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:enumeration value="Create"/>
						<xsd:enumeration value="Update"/>
						<xsd:enumeration value="Delete"/>
						<xsd:enumeration value="Load"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="EffectiveTimePeriodType">
		<xsd:sequence>
			<xsd:element name="StartDateTime" type="xsd:dateTime"/>
			<xsd:element name="EndDateTime" type="xsd:dateTime" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="EffectiveDatePeriodType">
		<xsd:sequence>
			<xsd:element name="StartDate" type="xsd:date"/>
			<xsd:element name="EndDate" type="xsd:date" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SequenceType">
		<xsd:sequence>
			<xsd:element name="SequenceIdentifier">
				<xsd:annotation>
					<xsd:documentation>Distinguishes this sequence of messages from others.</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="3"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="PositionIdentifier">
				<xsd:annotation>
					<xsd:documentation>Uniquely identifies and sequentially orders each message in the sequence.</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:positiveInteger">
						<xsd:totalDigits value="3"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="Size">
				<xsd:annotation>
					<xsd:documentation>Specifies the number of messages in the sequence.</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:positiveInteger">
						<xsd:totalDigits value="3"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
</xsd:schema>
