<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<xs:element name="Printers">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Printer" minOccurs="0"  maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Printer">
		<xs:complexType>
			<xs:attribute name="siteId" type="xs:string" use="required"/>
			<xs:attribute name="name" type="xs:string" use="required"/>
			<xs:attribute name="hostAddress" type="xs:string" use="required"/>
		</xs:complexType>
	</xs:element>
</xs:schema>
